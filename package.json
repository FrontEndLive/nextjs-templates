{
  "name": "generator-felive-nextjs",
  "version": "1.0.0-beta.15",
  "description": "Generates a NextJS project with TypeScript, Tailwind, Storybook, Jest, and more pre-configured",
  "homepage": "",
  "author": {
    "name": "Spencer Kline",
    "email": "dskline1@gmail.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "NextJS",
    "TypeScript",
    "Tailwind CSS",
    "Storybook",
    "Jest",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "mem-fs": "2.2.1",
    "prettier": "2.6.0",
    "yeoman-assert": "3.1.1",
    "yeoman-environment": "3.9.1",
    "yeoman-test": "6.3.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "5.6.1",
    "chalk": "5.0.1",
    "yosay": "2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    }
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest __tests__"
  },
  "repository": "frontendlive/generator-felive",
  "license": "MIT"
}
